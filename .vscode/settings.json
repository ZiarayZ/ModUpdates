{
    "eslint.enable": true,
    "eslint.lintTask.enable": true,
    "eslint.format.enable": true,
    "eslint.execArgv": null,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "typescriptreact"
    ],
    "eslint.workingDirectories": [
        "./src"
    ],
    "eslint.alwaysShowStatus": true
}